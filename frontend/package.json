{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:docker": "docker build -t chapellu/dofus-market-frontend:${version} -f deploy/Dockerfile .",
    "preview": "vite preview",
